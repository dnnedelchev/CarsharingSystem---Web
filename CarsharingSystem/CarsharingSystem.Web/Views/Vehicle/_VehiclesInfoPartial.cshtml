@model IList<CarsharingSystem.Web.ViewModels.Vehicle.ShowVehicleViewModel>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Марка</th>
            <th>Модел</th>
            <th>Места</th>
            <th>Пътувания</th>
            <th>Год. на производство</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].Label</td>
                <td>@Model[i].Model</td>
                <td>@Model[i].Seats</td>
                <td>@Model[i].TravelCounts</td>
                <td>@Model[i].ManufactureYear</td>
            </tr>
        }
    </tbody>
</table>